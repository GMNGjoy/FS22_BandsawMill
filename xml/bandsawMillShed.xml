                <spawnPlace startNode="plankSpawn9Start" endNode="plankSpawn9End" fillTypes="PLANKS"/>
            </spawnPlaces>
        </palletSpawner>

        <playerTrigger node="playerTrigger" />

        <sounds>
            <idle template="carpenterIdle" linkNode="0>" />
            <active template="carpenterWork" linkNode="0>" />
        </sounds>
    </productionPoint>

