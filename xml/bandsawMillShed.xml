                <spawnPlace startNode="plankSpawn1Start" endNode="plankSpawn1End" fillTypes="PLANKS"/>
                <spawnPlace startNode="plankSpawn2Start" endNode="plankSpawn2End" fillTypes="PLANKS"/>
